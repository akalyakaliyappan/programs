package program;

//superclass
public class singlelevel {
	void Message() {
		System.out.println("sending");
	}
  
}
class subclass extends singlelevel{
	void view() {
		System.out.println("received");
	}
}
class Main{
  public static void main(String[] args) {
	 subclass ref=new subclass();
	ref.Message();
	ref.view();
	}
}
